{
    "compilerOptions": {
        "target": "ES2019",
        "module": "commonjs",
        "outDir": "./bin",
        "sourceMap": false,
        "watch": true,
        "esModuleInterop": true,
        "noEmitOnError": true,
        "allowJs": true,
        "resolveJsonModule": true,
    },
    "exclude": [
        "node_modules",
        "bin"
    ]
}